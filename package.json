{
  "name": "angular-material-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "env": "ts-node set-env.ts",
    "start": "npm run env && ng serve",
    "deploy": "npm run env && ng build --base-href=\"https://hvhai.github.io/angular-material-playground/\"  && npx angular-cli-ghpages --dir=dist/angular-material-playground",
